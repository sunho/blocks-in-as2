<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">
  <category name="Clipevent" colour="#5b5ba5">
    <block type="onclipevent">
      <field name="type">enterFrame</field>
    </block>
    <block type="onclipevent">
      <field name="type">press</field>
    </block>
    <block type="onclipevent">
      <field name="type">rollOver</field>
    </block>
  </category>
  <category name="Actions" colour="#5ba5a5">
    <block type="stop">
      <value name="obj">
        <block type="this"></block>
      </value>
    </block>
    <block type="gotoandstop">
      <value name="frame">
        <block type="math_number">
          <field name="NUM">0</field>
        </block>
      </value>
      <value name="obj">
        <block type="_root"></block>
      </value>
    </block>
    <block type="play">
      <value name="obj">
        <block type="_root">
          <value name="next">
            <block type="variable">
              <field name="name">movieclip_name</field>
            </block>
          </value>
        </block>
      </value>
    </block>
  </category>
</xml>